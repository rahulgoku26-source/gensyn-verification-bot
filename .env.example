# ============================================
# PERFORMANCE SETTINGS
# ============================================
ENABLE_AUTO_VERIFY=true
AUTO_VERIFY_INTERVAL=5
AUTO_VERIFY_BATCH_SIZE=50
BATCH_SIZE=50
CACHE_TTL=300
MAX_CONCURRENT_VERIFICATIONS=10
DELAY_BETWEEN_CHECKS=100

# ============================================
# DATABASE
# ============================================
DB_PATH=./data/users.json
DB_BACKUP_ENABLED=true
DB_BACKUP_INTERVAL=1

# ============================================
# LOGGING
# ============================================
LOG_LEVEL=info
MAX_LOG_ENTRIES=1000
FAILED_LOG_PATH=./logs/failed.txt
SUCCESS_LOG_PATH=./logs/success.txt

# ============================================
# RATE LIMITING
# ============================================
VERIFY_COOLDOWN=60
LINK_COOLDOWN=30
EXPLORER_API_RATE=100

# ============================================
# FEATURES
# ============================================
FEATURE_DM_NOTIFICATIONS=true
FEATURE_ADMIN_COMMANDS=true
FEATURE_STATISTICS=true
